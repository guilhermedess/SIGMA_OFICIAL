@*@model USJT.Sigma.Model.Aluno*@

<head>
    <script src="~/scripts/progresso.js"></script>
    <link href="~/Content/progresso.css" rel="stylesheet" />
</head>

<div class="row">
    <div class="col-lg-11 col-md-11 col-sm-11">
        <fieldset>
            <legend>Pontos Obtidos</legend>
        </fieldset>
    </div>

    <div class="col-lg-11 col-md-11 col-sm-11">
        <div class="panel panel-default" style="width:100%; margin:0px">
            <div class="panel-heading">
                <h3 class="panel-title">
                    Progresso
                </h3>
            </div>

            <div class="panel-body" style="margin:0px; width:105%;">
                <div id="Main">
                    <a name="poll_bar" title="Total">Total</a> <span name="poll_val">@Model.Aluno.ProgressoTotal%</span><br />
                </div>

                <div id="Total">
                    <a name="poll_bar" title="Distribuição">Distribuição</a> <span name="poll_val">@Model.Aluno.ProgressoDistribuicao%</span><br />
                    <a name="poll_bar" title="Medidas&nbsp;de&nbsp;Tendência&nbsp;Central">Medidas&nbsp;de&nbsp;Tendência&nbsp;Central</a> <span name="poll_val">@Model.Aluno.ProgressoMedidasDeTendenciaCentral% </span><br />
                    <a name="poll_bar" title="Medidas&nbsp;de&nbsp;disperção">Medidas&nbsp;de&nbsp;disperção</a> <span name="poll_val">@Model.Aluno.ProgressoMedidasDeDispersao%  </span><br />
                    <a name="poll_bar" title="Amostragem&nbsp;e&nbsp;estimadores">Amostragem&nbsp;e&nbsp;estimadores</a> <span name="poll_val">@Model.Aluno.ProgressoAmostragemEstimadores%  </span><br />
                </div>

                <div id="Distribuição">
                    <a name="poll_bar" title="Introdução">Introdução</a> <span name="poll_val">@Model.Aluno.PDIntroducao% </span><br />
                    <a name="poll_bar" title="Pontos&nbsp;ou&nbsp;valores">Pontos ou valores</a> <span name="poll_val">@Model.Aluno.PDPontosValores% </span><br />
                    <a name="poll_bar" title="Classes&nbsp;ou&nbsp;intervalos">Classes ou intervalos</a> <span name="poll_val">@Model.Aluno.PDClassesIntervalos%  </span><br />
                    <a name="poll_bar" title="Frequência&nbsp;relativa&nbsp;ou&nbsp;percentual">Frequência relativa ou percentual</a> <span name="poll_val">@Model.Aluno.PDRelativaPercentual%  </span><br />
                    <a name="poll_bar" title="Acumulada&nbsp;simples&nbsp;ou&nbsp;absoluta">Acumulada simples ou absoluta</a> <span name="poll_val">@Model.Aluno.PDAcumuladaSimplesAbsoluta%  </span><br />
                    <a name="poll_bar" title="Acumulada&nbsp;relativa&nbsp;ou&nbsp;percentual">Acumulada relativa ou percentual</a> <span name="poll_val">@Model.Aluno.PDAcumuladaRelativaPercentual%  </span><br />
                    <a name="poll_bar" title="Frequências&nbsp;por&nbsp;pontos&nbsp;ou&nbsp;valores">Frequências por pontos ou valores</a> <span name="poll_val">@Model.Aluno.PDFreqPontosValores%  </span><br />
                    <a name="poll_bar" title="Frequência&nbsp;por&nbsp;classes&nbsp;ou&nbsp;intervalos">Frequência por classes ou intervalos</a> <span name="poll_val">@Model.Aluno.PDFreqClassesIntervalos%  </span><br />
                </div>

                <div id="Medidas&nbsp;de&nbsp;Tendência&nbsp;Central">
                    <a name="poll_bar" title="Introdução">Introdução</a> <span name="poll_val">@Model.Aluno.PTIntroducao%</span><br />
                    <a name="poll_bar" title="Média&nbsp;aritmética&nbsp;simples&nbsp;">Média aritmética simples</a> <span name="poll_val">@Model.Aluno.PTMediaSimples%</span><br />
                    <a name="poll_bar" title="Média&nbsp;aritmética&nbsp;e&nbsp;ponderada">Média aritmética ponderada</a> <span name="poll_val">@Model.Aluno.PTMediaPonderada%</span><br />
                    <a name="poll_bar" title="Moda">Moda</a> <span name="poll_val">@Model.Aluno.PTModa%</span><br />
                    <a name="poll_bar" title="Mediana">Mediana</a> <span name="poll_val">@Model.Aluno.PTMediana%</span><br />
                </div>
                @*
                    <div id="Medidas&nbsp;de&nbsp;disperção">
                        <a name="poll_bar" title="Dispersão">Dispersão</a> <span name="poll_val">60% </span><br />
                        <a name="poll_bar" title="Assimetria">Assimetria</a> <span name="poll_val">55% </span><br />
                        <a name="poll_bar" title="Curtose">Curtose</a> <span name="poll_val">46%  </span><br />
                    </div>

                    <div id="Amostragem&nbsp;e&nbsp;estimadores">
                        <a name="poll_bar" title="Experimento aleatório, espaço amostral e eventos">Experimento aleatório, espaço amostral e eventos</a> <span name="poll_val">60% </span><br />
                        <a name="poll_bar" title="Definição clássica e frequência relativa">Definição clássica e frequência relativa</a> <span name="poll_val">55% </span><br />
                        <a name="poll_bar" title="Tipos de eventos">Tipos de eventos</a> <span name="poll_val">46%  </span><br />
                        <a name="poll_bar" title="Axioma de probabilidade">Axioma de probabilidade</a> <span name="poll_val">46%  </span><br />
                    </div>*@
            </div>
        </div>
    </div>
</div>
